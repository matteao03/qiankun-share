{
  "name": "qiankun-vue-demo",
  "version": "1.0.0",
  "description": "用`qiankun`来实现`vue`技术栈的前端微服务",
  "main": "index.js",
  "scripts": {
    "install:app1": "cd app1 && npm install",
    "install:app2": "cd app2 && npm install",
    "install:main": "cd main && npm install",
    "install-all": "npm-run-all install:*",
    "start:app1": "cd app1 && npm run serve ",
    "start:app2": "cd app2 && npm run serve",
    "start:main": "cd main && npm run serve",
    "start-all": "npm-run-all --parallel start:*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {}
}
